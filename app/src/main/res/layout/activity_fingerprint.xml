<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="fragmentFingerprintVM"
            type="com.startogamu.zikobot.viewmodel.activity.fingerprint.ActivityFingerprintVM" />

    </data>

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.startogamu.zikobot.view.custom.ZikobotMessageView
            android:id="@+id/zmv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:zmv_message="Identifie la chanson"
            android:onClick="@{fragmentFingerprintVM.startIdentification}"
            android:layout_centerInParent="true" />
    </RelativeLayout>


</layout>